class Solution {
    public int maximumPopulation(int[][] logs) {
        int arr[]=new int[101];
        int max=0;
        int maxyear=0;
      
        for(int j=0;j<logs.length;j++){      
            arr[logs[j][0]-1950]++;
            arr[logs[j][1]-1950]--;
        }
        
        for(int i=1;i<101;i++){
            arr[i]=arr[i-1]+arr[i];
        }
        
        for(int i=0;i<arr.length;i++){
            if(arr[i]>max){
                max=arr[i];
                maxyear=i+1950;
            }      
        }
        return maxyear;
    }
}
